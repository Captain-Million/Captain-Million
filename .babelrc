{
  "passPerPreset": true,
  "presets": [
    { "plugins": ["./babel-relay-plugin"] },
    "react",
    "es2015",
    "stage-0"
  ],

  "plugins": ["react-hot-loader/babel"],
  "env": {
    "production": {
      "presets": [
        { "plugins": ["./babel-relay-plugin"] },
        "es2015",
        "react",
        "react-optimize",
        "es2015-native-modules",
        "stage-0"
      ],
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-constant-elements"
      ]
    }
  }
}
